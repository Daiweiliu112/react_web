             <Route path=\"/Project1\" component={Project1} />\n              <Route path=\"/Project2\" component={Project2} />\n              <Route path=\"/Project3\" component={Project3} />\n              <Route path=\"/UW\" component={UW} />\n              <Route path=\"/Programmer\" component={Programmer} />\n              <Route path=\"/History\" component={History} />\n              <Route path='/Github' component={() => {\n                window.location.replace('https://github.com/Daiweiliu112');\n                return null;\n              }} />\n              <Route path='/Linkedin' component={() => {\n                window.location.replace('https://linkedin.com/in/daiweiliu112/');\n                return null;\n              }} />\n              <Route path ='/Websites' component={Websites}/>\n\n            </Switch>\n            <Footer />\n\n\n\n          </Container>\n        </Router>\n\n\n      </div>\n    )\n  }\n}\n\nexport default App;\n","/home/d239liu/work/react_web/src/reportWebVitals.js",[],"/home/d239liu/work/react_web/src/comp/js/Header.js",["124","125"],"import React from 'react';\nimport {BrowserRouter as Router, Route, Link} from 'react-router-dom'\nimport Container from 'react-bootstrap/Container';\nimport Navbar from 'react-bootstrap/Navbar';\nimport NavbarBrand from 'react-bootstrap/NavbarBrand';\nimport { Nav } from 'react-bootstrap';\n\n// have to import the css before the bootstrap will work\nimport 'bootstrap/dist/css/bootstrap.min.css';\n\n\n\nclass Header extends React.Component {\n  constructor(props){\n    super(props);\n    this.state = {\n      // Router links to be implemented\n      title: 'David Liu',\n      headerLinks:[\n        {title:'Project 1', path:'/comp/js/Project1'},\n        {title:'Project 2', path:'/comp/js/Project2'},\n        {title:'Project 3', path:'/comp/js/Project3'}\n      ],\n\n      project1: {\n        title: \"Tech for Teletherapy\",\n        text: \"Improving online speech therapy\"\n      },\n\n